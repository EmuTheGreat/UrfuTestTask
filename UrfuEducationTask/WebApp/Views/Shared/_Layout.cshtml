<!DOCTYPE html>
<html lang="ru">
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title – UrFU Edu</title>
    <meta name="viewport" content="width=device-width, initial-scale=1" />

    <!-- Bootstrap CSS (CDN) -->
    <link
        href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css"
        rel="stylesheet"
        integrity="sha384-XLK+YhQgXg67frdS/5hMtvQzQi95aMaU6aF5NQoR67HFUYMR1qYv7SS9yIytKTOo"
        crossorigin="anonymous">

    <!-- Ваши дополнительные стили, если есть -->
    <link href="~/css/site.css" rel="stylesheet" />
</head>
<body>
<nav class="navbar navbar-expand-lg navbar-light bg-light">
    <!-- навигация -->
</nav>

<div class="container mt-4">
    @RenderBody()
</div>

<!-- Bootstrap JS + Popper (CDN) -->
<script
    src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js"
    integrity="sha384-ENjdO4Dr2bkBIFxQpeoCw1tFQmK7oYJgU52b8J4KiN1p0c5+XxAn7V+zt+JWnkgQ"
    crossorigin="anonymous"></script>

<!-- Ваш собственный скрипт -->
<script src="~/js/app.js"></script>
@RenderSection("Scripts", required: false)
</body>

</html>
